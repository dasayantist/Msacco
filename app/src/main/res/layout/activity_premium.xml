<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:id="@+id/scrollView1"
    android:layout_width="match_parent"
    android:background="#AAA8A8"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/activity_premium"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        tools:context=".Premium"
        android:weightSum="1">

        <android.support.design.card.MaterialCardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5sp"
            android:layout_marginTop="10sp"
            android:layout_marginEnd="5sp"
            app:cardCornerRadius="10sp"
            app:cardMaxElevation="15sp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20sp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Name : " />

                    <TextView
                        android:id="@+id/txt_Fname"
            android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="shadrack Ngetick" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="AC NO : " />

                    <TextView
                        android:id="@+id/txt_acno"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="ac_no" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Loan Type : " />

                    <TextView
                        android:id="@+id/txt_Ltype"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="10000039" />
                </LinearLayout>

            </LinearLayout>


        </android.support.design.card.MaterialCardView>

        <android.support.design.card.MaterialCardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5sp"
            android:layout_marginTop="10sp"
            android:layout_marginEnd="5sp"
            app:cardCornerRadius="10sp"
            app:cardMaxElevation="15sp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20sp"
                    android:layout_marginTop="10sp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                            android:text="TOTAL LOAN" />

                        <TextView
                        android:layout_width="wrap_content"
                            android:id="@+id/txt_total"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20sp"
                            android:text="@string/_1000"
                            android:textStyle="bold" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="BALANCE" />

                        <TextView
                            android:id="@+id/txt_principal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20sp"
                            android:text="@string/_1000"
                            android:textStyle="bold" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="INTEREST" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/txt_interest"
                        android:layout_marginStart="20sp"
                        android:text="@string/_1000"
                        android:textStyle="bold" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                        android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="PENALTY" />

                        <TextView
                            android:id="@+id/txt_penalty"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20sp"
                            android:text="@string/_100"
                            android:textStyle="bold" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/other_charges" />

                        <TextView
                            android:id="@+id/txt_O_charges"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20sp"
                            android:text="@string/other_c"
                            android:textStyle="bold" />

                    </LinearLayout>

                    <android.support.design.widget.TextInputLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <EditText
                            android:id="@+id/etLAmount"
                            android:layout_width="wrap_content"
                            android:layout_height="35dp"
                            android:layout_marginTop="16dp"
                            android:layout_marginBottom="16dp"
                            android:background="#D5EEE9"
                            android:ems="10"
                            android:hint="Enter Amount to Pay"
                            android:inputType="phone"
                            android:textColor="@android:color/black"
                            android:textStyle="normal|bold" />

                    </android.support.design.widget.TextInputLayout>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <EditText
                        android:id="@+id/etPhoneNumber"
                        android:layout_width="wrap_content"
                        android:layout_height="35dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginBottom="16dp"
                        android:background="#D5EEE9"
                        android:ems="10"
                        android:hint="@string/enter_phone_no"
                        android:inputType="phone"
                        android:textColor="@android:color/black"
                        android:textStyle="normal|bold" />


                    <Button
                        android:id="@+id/bPay"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="30dp"
                        android:background="#cc0505"
                        android:enabled="false"
                        android:text="@string/pay"
                        android:textSize="18sp"
                        android:textStyle="normal|bold" />
                </LinearLayout>

                <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Attempts:"
                        android:textSize="24sp"
                        android:textStyle="normal|bold" />

                    <TextView
                        android:id="@+id/textView_attemp_Count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:background="#000"
                        android:textColor="#fff"
                        android:textSize="24sp"
                        android:textStyle="normal|bold" />
                    <!--android:id="@+id/textView_attemt_Count"-->
                </LinearLayout>

                <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:gravity="center_horizontal"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/bConfirm"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp"
                        android:background="#cc0505"
                        android:text="@string/confirm_last_pay"
                        android:textSize="18sp"
                        android:textStyle="normal|bold" />

                    <Button
                        android:id="@+id/bAccess"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginBottom="10dp"
                        android:background="#cc0505"
                        android:text="@string/check_transaction"
                        android:textSize="18sp"
                        android:textStyle="normal|bold" />


                </LinearLayout>
            </LinearLayout>

        </android.support.design.card.MaterialCardView>


    </LinearLayout>
